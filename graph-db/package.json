    "test:watch": "jest --watchAll",
    "test-coverage": "jest --coverage",
    "ci-test": "jest --coverage --config jest.config.junit_output.js",
    "bootstrap-local": "serverless invoke local -f bootstrapGremlinServer -e CMR_ROOT=\"http://cmr.uat.earthdata.nasa.gov\" -e PAGE_SIZE=2000 -e GREMLIN_URL=\"ws://localhost:8182/gremlin\" --stage=sit",
    "deploy": "./node_modules/.bin/sls deploy --config serverless.yml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run -c video=false",
    "cypress:ci": "NODE_ENV=test_cypress start-server-and-test start:ci http-get://localhost:8080 cypress:run",
    "cypress:prepare-ci": "bin/cypress-prepare-ci"
  },
