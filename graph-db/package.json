    "serverless": "^2.38.0",
    "serverless-webpack": "^5.5.1",
    "webpack": "^5.39.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
